    <Declaration><![CDATA[FUNCTION_BLOCK UNIT_Blikkenvullijn IMPLEMENTS  ITF_UNIT_Blikkenvullijn, ITF_PackMLV2
VAR
	fb_mem									: STRUCT_UNIT_Blikkenvullijn;
	//PackML									: PackML_StateMachine;
	
	//Custom logic
	EM_Can_Supply 							: EM_Can_Supply;		//Replace with actual block not function
	//CM_TurningDevice						: CM_Motor;
	CM_Outfeed_ProductionLineSensor			: CM_Sensor;
	
	//PackML machine's
	EM_Filling								: EM_Machine;		//Replace with actual block not function
	EM_CheckWeigher							: EM_Machine;		//Replace with actual block not function
	EM_BottomSeal							: EM_Machine;		//Replace with actual block not function
	EM_Labeller								: EM_Machine;		//Replace with actual block not function
	EM_TurningDevice						: EM_Machine;		//Replace with actual block not function
	EM_StampUnit							: EM_Machine;		//Replace with actual block not function
	EM_SealShrink							: EM_Machine;		//Replace with actual block not function
	
	//Transport logic
	EM_Infeed_CheckWeigher					: EM_Transport;
	EM_Transport_Checkweigher_BottomSeal	: EM_Transport_Mechanische_Stop;
	EM_Transport_BottomSeal_Labeller		: EM_Transport_Mechanische_Stop;
	EM_Transport_TurningDevice_StampUnit	: EM_Transport;
	EM_Transport_StampUnit_SealShrink		: EM_Transport_Mechanische_Stop;
	
	//Management Logic
	AlarmHandler 							: AlarmHandler;
	ModeSelect								: ModeSelect;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[(*
