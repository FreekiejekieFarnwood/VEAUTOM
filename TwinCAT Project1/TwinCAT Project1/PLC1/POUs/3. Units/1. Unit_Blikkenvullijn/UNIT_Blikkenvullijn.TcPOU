EM_Transport_TurningDevice_StampUnit.ZeroAllVar();
EM_StampUnit.ZeroAllVar();
EM_Transport_StampUnit_SealShrink.ZeroAllVar();
EM_SealShrink.ZeroAllVar();
//HMI values
//INSERT HERE
